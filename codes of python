# Modifie List Questions (Modify List)
# -----------------------------------------------------------------------------
print("\n--- Modify List Questions ---")

# Question 1) Modify the element at index 1 of list ["ronit", "rupa", "salman"] to "rupesh".
print("\n1) Modifying an element by index:")
a_mod1 = ["ronit", "rupa", "salman"]
a_mod1[1] = "rupesh"
print(a_mod1)
# Output 1:
# 1) Modifying an element by index:
# ['ronit', 'rupesh', 'salman']

# Question 2) Modify the element at index 1 of list [1, 2, 3, 4] to 3.
print("\n2) Modifying an element (numbers):")
b_mod2 = [1, 2, 3, 4]
b_mod2[1] = 3
print(b_mod2)
# Output 2:
# 2) Modifying an element (numbers):
# [1, 3, 3, 4]

# Question 3) Append "rupali" to the list ["ronit", "ram", "sailu"].
print("\n3) append() an item:")
a_mod3 = ["ronit", "ram", "sailu"]
a_mod3.append("rupali")
print(a_mod3)
# Output 3:
# 3) append() an item:
# ['ronit', 'ram', 'sailu', 'rupali']

# Question 4) Append "ronit" to the list ["ram", "hari", "sita"].
print("\n4) append() another item:")
a_mod4 = ["ram", "hari", "sita"]
a_mod4.append("ronit")
print(a_mod4)
# Output 4:
# 4) append() another item:
# ['ram', 'hari', 'sita', 'ronit']

# Question 5) Insert "ronit" at the beginning (index 0) of the list ["ram", "hari", "krish"].
print("\n5) insert() an item at a specific position:")
a_mod5 = ["ram", "hari", "krish"]
a_mod5.insert(0, "ronit") # Insert at the beginning
print(a_mod5)
# Output 5:
# 5) insert() an item at a specific position:
# ['ronit', 'ram', 'hari', 'krish']

# Question 6) Remove the first occurrence of "rupa" from the list ["ronit", "ram", "rajak", "rupa"].
print("\n6) remove() an item by value:")
a_mod6 = ["ronit", "ram", "rajak", "rupa"]
a_mod6.remove("rupa")
print(a_mod6)
# Output 6:
# 6) remove() an item by value:
# ['ronit', 'ram', 'rajak']

# Question 7) Demonstrate pop() by removing elements at index 0, then index 1 (of modified list), then index 0 (again), then the last item.
print("\n7) pop() an item by index (and default):")
a_mod7 = ["ronit", "ram", "rajak", "rupa"]
print("Original list:", a_mod7)
item_popped = a_mod7.pop(0)
print("Popped item:", item_popped, "Remaining list:", a_mod7)
item_popped = a_mod7.pop(1)
print("Popped item:", item_popped, "Remaining list:", a_mod7)
item_popped = a_mod7.pop(0)
print("Popped item:", item_popped, "Remaining list:", a_mod7)
item_popped = a_mod7.pop()
print("Popped item:", item_popped, "Remaining list:", a_mod7)
# Output 7:
# 7) pop() an item by index (and default):
# Original list: ['ronit', 'ram', 'rajak', 'rupa']
# Popped item: ronit Remaining list: ['ram', 'rajak', 'rupa']
# Popped item: rajak Remaining list: ['ram', 'rupa']
# Popped item: ram Remaining list: ['rupa']
# Popped item: rupa Remaining list: []

# Question 8) Clear all elements from the list ["item1", "item2", "item3"].
print("\n8) clear() a list:")
a_mod8 = ["item1", "item2", "item3"]
print("Before clear:", a_mod8)
a_mod8.clear()
print("After clear:", a_mod8)
# Output 8:
# 8) clear() a list:
# Before clear: ['item1', 'item2', 'item3']
# After clear: []

